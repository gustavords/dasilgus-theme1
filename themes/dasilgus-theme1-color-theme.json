{


	

	"name": "dasilgus-theme1",
	"colors": {
		"editor.background": "#E5EBC3", //E4FFC8 //F0E9E2 //F5CE88 // F2E9D0 //FFE2EE
		"editor.foreground": "#96572A", // 96572A, D6794D //any writting thats not code or comments
        "list.activeSelectionIconForeground": "#df1515" //the icon selected when in quickmenu
	},
	"tokenColors": [

			
		// {
		// 	"name": "Brackets",
		// 	"scope": [
		// 		"bracket",
		// 		"punctuation.definition.parameters.begin.js",
		// 		"meta.parameters.js",
		// 		"meta.function.js",
		// 		"source.js",
		// 	],
		// 	"settings": {
